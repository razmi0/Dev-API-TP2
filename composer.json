{
  "require": {
    "slim/slim": "^4.0.0",
    "slim/psr7": "^1.0.0"
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public public/index.php"
  },
  "config": {
    "process-timeout": 0
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Model\\Dao\\": "model/dao",
      "Model\\Entity\\": "model/entity",
      "HTTP\\": "http/",
      "Middleware\\": "middleware/",
      "Middleware\\Validators\\": "middleware/validator/"
    }
  }
}
